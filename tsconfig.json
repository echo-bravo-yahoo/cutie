  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es5",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "module": "nodenext",
    "moduleResolution": "nodenext"
  },
  "include": ["./src/**/*"]
}
